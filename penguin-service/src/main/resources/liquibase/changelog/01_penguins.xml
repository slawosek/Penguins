<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="1" author="slawosek">

        <createTable tableName="penguins">
            <column name="taxon_key" type="VARCHAR(1024)">
                <constraints primaryKey="true" primaryKeyName="penguins__PK"/>
            </column>
            <column name="scientific_name" type="VARCHAR(1024)"/>
            <column name="number_of_occurrences" type="VARCHAR(1024)"/>
            <column name="accepted_scientific_name" type="VARCHAR(1024)"/>
            <column name="species" type="VARCHAR(1024)"/>
        </createTable>

    </changeSet>

</databaseChangeLog>